
<li>
    <div class="heatmap-row">
        {{createRow this}}
        <div class="pull-right">
            {{#each values}}
                {{showValue this}}
            {{/each}}
        </div>
    </div>
    <ul class="tree heatmap-closed">
        {{#if detailData}}
            <div class="heatmap-detail">
                {{#each detailData}}
                    <div>
                    {{addDetail this}}
                    </div>
                {{/each}}
            </div>
        {{/if}}
        {{#each childrenHTML}}
            {{this}}
        {{/each}}
    </ul>
</li>
