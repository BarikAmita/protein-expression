{{#each children}}
<li>
    <div class="heatmap-row">
        <p class="heatmap-rowLabel tree-toggler">
            {{#createIcon this}}
            {{/createIcon}}
            <span class="rowLabel">{{rowLabel}}</span>
            <span><a href="{{linkURL}}" target="_blank">{{linkLabel}}</a></span>
        </p>
        <div class="pull-right">
            {{#forCreateCircle values}}
                <div class="heatmap-column {{columnClass}}", style="width:{{columnWidth}}">
                    <i class="heatmap-circle {{circleColorClass}}" style="background-color: {{circleColorStyle}}"></i>
                </div>
            {{/forCreateCircle}}
        </div>
    </div>
    <ul class="tree heatmap-closed">
        {{#if detailData}}
            <div class="heatmap-detail">
                {{#each detailData}}
                    <div>
                    {{addDetail this}}
                    </div>
                {{/each}}
            </div>
        {{/if}}
        {{#if children}}
            {{> create-children}}
        {{/if}}
    </ul>
</li>
{{/each}}
