<ul class="tree">
    {{#each children}}
    <li>
        <div>
            <p class="tree-toggler" style="display:inline-block;margin-bottom:0px;word-break:break-all;"><span class="glyphicon glyphicon-plus"></span><span>{{rowLabel}}</span></p>
            <div class="pull-right">
                {{#forCreateIcons values}}
                    <div class="column {{columnClass}}", style="width:{{columnWidth}}">
                        <i class="circle {{circleColor}}"></i>
                    </div>
                {{/forCreateIcons}}
            </div>
        </div>
        {{#if children}}
            {{> create-ul}}
        {{/if}}
    </li>
    {{/each}}
</ul>
