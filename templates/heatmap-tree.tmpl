<ul class="tree">
    {{#each children}}
    <li>
        <div class="heatmap-row" style="overflow:hidden">
            <p class="tree-toggler" style="display:inline-block;margin-bottom:0px;word-break:break-all;"><span class="glyphicon glyphicon-plus"></span><span>{{rowLabel}}</span><span><a style="margin-left:5px; font-size:12px;" href="{{linkURL}}" target="_blank">{{linkLabel}}</a></span></p>
            <div class="pull-right">
                {{#forCreateIcons values}}
                    <div class="heatmap-column {{columnClass}}", style="width:{{columnWidth}}">
                        <i class="circle {{circleColor}}"></i>
                    </div>
                {{/forCreateIcons}}
            </div>
        </div>
        {{#if children}}
            {{> create-ul}}
        {{/if}}
    </li>
    {{/each}}
</ul>
